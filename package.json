{
    "name": "formlink",
    "description": "Fullstack form handler library.",
    "version": "0.0.11",
    "type": "module",
    "author": "Jerome Thayananthajothy <tjthavarshan@gmail.com>",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.js"
        },
        "./dist/*": "./dist/*",
        "./package.json": "./package.json"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/formlinkjs/formlink.git"
    },
    "bugs": {
        "url": "https://github.com/formlinkjs/formlink.git/issues"
    },
    "homepage": "https://formlinkjs.com",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc && vite build",
        "lint": "eslint --ext .ts,.js src",
        "test": "vitest"
    },
    "dependencies": {
        "axios": "^1.5.1",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^11.1.5",
        "@types/lodash": "^4.14.199",
        "@types/node": "^20.8.6",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "axios-mock-adapter": "^1.22.0",
        "eslint": "^8.51.0",
        "eslint-plugin-mdx": "^2.2.0",
        "jsdom": "^22.1.0",
        "rollup-plugin-typescript-paths": "^1.4.0",
        "typescript": "^5.2.2",
        "vite": "^5.0.11",
        "vitest": "^0.34.6"
    }
}
